{
  "name": "FLConsole",
  "description": "Shows traces, warnings, and errors from SWF on the Console in Chrome Developer Tools.",
  "version": "0.0.1",
  "icons": {
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "images/icon_48_inactive.png",
    "default_title": "FLConsole"
  },
  "content_scripts": [
    {
      "matches": [
        "file:///*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/jquery-1.6.min.js",
        "scripts/contents.js"
      ],
      "run_at": "document_start"
    }
  ],
  "plugins": [
    {
      "path": "plugins/flconsole.dll"
    }
  ],
  "permissions": [
    "tabs",
    "file:///*",
    "http://*/*",
    "https://*/*"
  ]
}